{
  "name": "spacetail-v2",
  "version": "0.0.1",
  "description": "Excalibur TypeScript template for Vite",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsx watch ./server/index.ts\"",
    "start": "npm run dev",
    "build": "tsc && vite build && vite build --ssr ./server/index.ts",
    "test": "npm run build && ex-test -d ./dist -t ./test/test.js",
    "serve": "cross-env NODE_ENV=production node ./dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/excaliburjs/template-ts-vite.git"
  },
  "keywords": [
    "excalibur",
    "excaliburjs",
    "vite",
    "game-engine"
  ],
  "author": "Erik Onarheim",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/excaliburjs/template-ts-vite/issues"
  },
  "homepage": "https://github.com/excaliburjs/template-ts-vite#readme",
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "excalibur": "0.29.3",
    "tsx": "^4.15.6",
    "typescript": "5.4.5",
    "vite": "5.3.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@excaliburjs/testing": "^0.25.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "ws": "^8.17.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  }
}
